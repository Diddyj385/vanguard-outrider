{
    "compilerOptions": {
        "outDir": "./game/",
        "sourceMap": false,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "files": [
        "./source/index.d.ts"
    ],
    "include": [
        "./source/*",
        "./source/**/*",
        "./static/*",
        "./static/**/*"
    ]
}